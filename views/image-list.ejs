<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
  <%- include('./header.ejs') %>
  <h1>Stored Images</h1>
  <p><%=(typeof alertMsg!='undefined')? alertMsg:''%></p>
  <% for(var i=0; i < data.length; i++) { %>
    <div>
      <img src="/images/<%= data[i].image_name %>" height="300" alt=<%= data[i].image_name %>>
      <div><%= data[i].image_name %></div>
      <form action="/delete-image/<%= data[i].id %>" method="POST">
        <button type="submit">Delete</button>
      </form>
    </div>
    <hr>
  <% } %>
</body>
</html>
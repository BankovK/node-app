<form action="/register" method="POST">
  <h1>Register</h1>
  <p><%=(typeof alertMsg!='undefined')? alertMsg:''%></p>
  <div>
    <input name="username" placeholder="Name">
  </div>
  <div>
    <input type="password" name="password" placeholder="Password">
  </div>
  <div>
    <button type="submit">Register</button>
  </div>
</form>